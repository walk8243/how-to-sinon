{
  "name": "how-to-sinon",
  "version": "1.0.0",
  "description": "Sinon.JSを使ったテストの書き方",
  "main": "index.js",
  "scripts": {
    "build": "yarn run tsc",
    "test": "yarn run nyc mocha test/*.ts"
  },
  "repository": "git@github.com:walk8243/how-to-sinon.git",
  "author": "walk8243 <walk8243@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.22",
    "@types/sinon": "^9.0.4",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2",
    "ts-node": "^8.10.2"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "*.d.ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
